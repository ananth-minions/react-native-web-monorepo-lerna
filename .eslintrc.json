{
	"extends": [
		"rallycoding"
	],
	"env": {
		"jest": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"modules": true,
			"classes": true
		}
	},
	"rules": {
		"react/sort-comp": [1, {
			"order": [
				"static-methods",
				"lifecycle"
			]
		}],
		"arrow-body-style": 0,
		"max-len": 0,
		"no-nested-ternary": 0,
		"no-underscore-dangle": 0,
		"prefer-template": 0,
		"global-require": 0,
		"import/no-unresolved": 0,
		"import/no-extraneous-dependencies": 0,
		"key-spacing": 0,
		"camelcase": 0,
		"padded-blocks": 0,
		"default-case": 0,
		"comma-dangle": 0,
		"indent": ["error", "tab", { "SwitchCase": 1 }]
	},
	"globals": {
		"fetch": false
	}
}
